import java.util.*;
class Array1
{
public static void main(String arg[])
{
int a[]=new int[5];
Scanner sc=new Scanner(System.in);
System.out.println("enter the array values");
for (int i=0;i<=4;i++)
{
a[i]=sc.nextInt();
}
System.out.println("The Array values are");
for(int i=0;i<=4;i++)
{
System.out.println(a[i]);
}
}
}
