class Area
{
	int b,h;
	float area;
	void get(int x,int y)
	{
		b=x;
		h=y;
	}
	void cal()
	{
		area=(1/2)*(b*h);
	}
	void disp()
	{
		System.out.println("output="+area);
	}
}
class AreaofTriangle1
{
	public static void main(String arg[])
	{
		Area obj=new Area();	
		obj.get(6,2);
		obj.cal();
		obj.disp();
	}
}
