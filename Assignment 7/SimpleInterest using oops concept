class SimpleInterest
{
	double p;
	double t;
	double r;
	double si;
	void get(int x,int y,int z)
	{
		p=x;
		t=y;
		r=z;
	}
	void cal()
	{
		si=(p*t*r)/100;
	}
	void disp()
	{
		System.out.println("output="+si);
	}
}
class SimpleInterestTest
{
	public static void main(String arg[])
	{
		SimpleInterest obj=new SimpleInterest();	
		obj.get(13000,2,10);
		obj.cal();
		obj.disp();
	}
}
