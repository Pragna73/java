import java.util.*;
class Perfect
{
public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
int n,i,sum=0;
System.out.println("enter a number:");
n=sc.nextInt();
for(i=1;i<n;i++)
{
if(n%i==0)
{
sum=sum+i;
}
}
if(sum==n)
{
System.out.println("perfect number");
}
else
{
System.out.println("not a perfect number");
}
}
}

