import java.util.*;
class Array4
{
public static void main(String arg[])
{
int a[]=new int[5],sum=0;
Scanner sc=new Scanner(System.in);
System.out.println("the array values are:");
for(int i=0;i<=4;i++)
{
a[i]=sc.nextInt();
sum=sum+a[i];
}
System.out.println("the sum of array values are:"+sum);
}
}
