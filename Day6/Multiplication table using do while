import java.util.*;
class Table2
{
public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
int n,m;
System.out.println("enter the number the table u have to display");
n=sc.nextInt();
System.out.println("enter the number the till which u have to display");
m=sc.nextInt();
do
{
System.out.println(n+"*"+m+"="+n*m);
n++;
}
while(n<m);
}
}
