import java.util.*;
class Table
{
public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
int n,i,m;
System.out.println("enter the number for which you have to get the table");
n=sc.nextInt();
System.out.println("enter the number till which u hav to multiply");
m=sc.nextInt();
for(i=0;i<=m;i++)
{
System.out.println(n+"*"+i+"="+n*i);
}
}
}
