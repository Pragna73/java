import java.util.*;
class Factorial
{
public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
int n,i,fact=1;
System.out.println("enter the number till which u have to want to display");
n=sc.nextInt();
for(i=1;i<n;i++)
{
 fact=fact*i;
}
System.out.println("the factorial="+fact);
}
}
